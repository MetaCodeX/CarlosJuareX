Algoritmo LeerNumeros
Dimension numeros[50] 
Definir cantidadNumeros Como  entero
    cantidadNumeros <- 1
    
    Escribir "Ingrese números (999 para terminar):"
    Leer numero
    
    Mientras numero <> 999 Hacer
        numeros[cantidadNumeros] <- numero
        cantidadNumeros <- cantidadNumeros + 1
        Leer numero
    FinMientras
    
 
	
	
	Escribir Sin Saltar "Números pares: "
    Para i <- 1 Hasta cantidadNumeros - 1 Hacer
        Si numeros[i] MOD 2 = 0 Entonces
            Escribir Sin Saltar " ", numeros[i]
        FinSi
    FinPara
    Escribir " " 
	Escribir " " 
    Escribir Sin Saltar "Números impares: "
    Para i <- 1 Hasta cantidadNumeros - 1 Hacer
        Si numeros[i] MOD 2 <> 0 Entonces
            Escribir Sin Saltar " ", numeros[i]
        FinSi
    FinPara
	Escribir " "
	Escribir " " 
	
FinAlgoritmo
